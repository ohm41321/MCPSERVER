{
  "name": "mcp-servers",
  "version": "1.0.0",
  "description": "Multi-Server MCP (Model Context Protocol) implementation with database integration",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev:a": "nodemon --exec ts-node src/server-a.ts",
    "dev:b": "nodemon --exec ts-node src/server-b.ts",
    "start:a": "node dist/server-a.js",
    "start:b": "node dist/server-b.js",
    "db:setup": "node dist/database/setup.js",
    "db:migrate": "node dist/database/migrate.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "pg": "^8.11.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "ai",
    "tools",
    "multi-server"
  ],
  "author": "MCP Server Team",
  "license": "MIT"
}